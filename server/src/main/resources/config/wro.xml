<groups xmlns="http://www.isdc.ro/wro" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.isdc.ro/wro wro.xsd">
    <group name='vendor'>
        <js>classpath:META-INF/resources/webjars/jquery/1.9.0/jquery.js</js>

        <js>classpath:META-INF/resources/webjars/bootstrap/2.3.2/js/bootstrap.js</js>

        <js>classpath:META-INF/resources/webjars/underscorejs/1.4.4/underscore.js</js>
        <js>classpath:META-INF/resources/webjars/underscore.string/2.3.0/underscore.string.js</js>

        <js>classpath:META-INF/resources/webjars/bootstrap-datepicker/1.0.1/js/bootstrap-datepicker.js</js>
        <js>classpath:META-INF/resources/webjars/bootstrap-datepicker/1.0.1/js/locales/**</js>

        <js>classpath:META-INF/resources/webjars/momentjs/2.2.1/moment.js</js>
        <js>classpath:META-INF/resources/webjars/momentjs/2.2.1/lang/en-ca.js</js>

        <!-- Rest and Angularjs -->
        <js>classpath:META-INF/resources/webjars/angularjs/1.0.8/angular.js</js>
        <js>classpath:META-INF/resources/webjars/angularjs/1.0.8/angular-resource.js</js>
        <js>classpath:META-INF/resources/webjars/angular-strap/0.7.4/angular-strap.js</js>

        <js>classpath:META-INF/resources/webjars/lodash/1.2.0/lodash.js</js>
        <js>classpath:META-INF/resources/webjars/restangular/1.1.3/restangular.js</js>

        <js>classpath:META-INF/resources/webjars/angular-ui/0.4.0/angular-ui.js</js>
        <js>classpath:META-INF/resources/webjars/angular-ui-bootstrap/0.5.0/ui-bootstrap-tpls.js</js>

        <css>classpath:META-INF/resources/webjars/bootstrap/2.3.2/css/bootstrap.css</css>
        <css>classpath:META-INF/resources/webjars/bootstrap/2.3.2/css/bootstrap-responsive.css</css>

        <css>classpath:META-INF/resources/webjars/bootstrap-datepicker/1.0.1/css/datepicker.css</css>

        <css>classpath:META-INF/resources/webjars/font-awesome/3.2.1/css/font-awesome.css</css>

        <js>/javascript/vendor/**</js>
        <css>/less/vendor/**</css>
    </group>
    <group name='app'>
        <js>/javascript/app/**</js>
        <css>/less/app/**</css>
    </group>
</groups>